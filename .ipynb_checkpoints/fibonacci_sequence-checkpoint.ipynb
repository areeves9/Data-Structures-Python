    "def fib(n):\n",
    "    \"\"\"\n",
    "    Get the next number in the sequence by adding\n",
    "    the two previous elements.\n",
    "    \"\"\"\n",
    "    if n <= 1:\n",
    "        return n\n",
    "    else:\n",
    "        return(fib(n-1) + fib(n-2))\n",
    "    \n",
    "fib(10)"
